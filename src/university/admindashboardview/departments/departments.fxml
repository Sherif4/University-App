<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="DepartmentsAP" prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="university.admindashboardview.departments.DepartmentsController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="850.0">
         <children>
            <Label text="Departments" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Segoe UI Semibold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <Button fx:id="deldept" mnemonicParsing="false" onAction="#btnDeleteDept" stylesheets="@../../resources/admindashboard/style.css" text="Delete Department" textAlignment="CENTER">
                     <styleClass>
                        <String fx:value="search-container" />
                        <String fx:value="primary-color" />
                     </styleClass>
                     <font>
                        <Font name="Segoe UI Semibold" size="15.0" />
                     </font></Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <TableView fx:id="DeptsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="463.0" prefWidth="830.0">
              <columns>
                  <TableColumn fx:id="T_id" prefWidth="96.0" text="ID" />
                <TableColumn fx:id="T_name" prefWidth="462.0" text="Name" />
                  <TableColumn fx:id="T_avgGPA" minWidth="0.0" prefWidth="271.0" text="Average GPA" />
              </columns>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
